{
	"compilerOptions": {
		"allowJs": true,
		"baseUrl": ".",
		"esModuleInterop": true,
		"importHelpers": true,
		"inlineSources": true,
		"isolatedModules": true,
		"module": "ESNext",
		"moduleResolution": "node",
		"noEmit": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"target": "ES6",
		"lib": ["DOM", "ES5", "ES6", "ES7"]
	},
	"include": ["**/*.ts"]
}
